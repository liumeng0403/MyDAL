    <DotNetCliToolReference Include="dotnet-xunit" Version="$(xUnitVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MyDAL.Test\MyDAL.Test.csproj" />
  </ItemGroup>

</Project>
